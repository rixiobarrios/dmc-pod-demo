{
  "welcomeFile": "/PoDPlugins/index.html",
  "authenticationMethod": "none",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/PoDPlugins/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    },
    {
      "source": "^/external/api/(.*)$",
      "target": "$1",
      "destination": "EXTERNAL_API_DEST",
      "csrfProtection": false,
      "authenticationType": "none"
    }
  ]
}